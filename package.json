{
  "name": "starter-basic",
  "version": "0.1.0",
  "description": "Basic JS + CSS starter",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build:dev",
    "build": "echo 'npm run build:[env]\ndev/stage/prod'",
    "build:dev": "webpack --config ./webpack.config.js --env.name dev",
    "build:stage": "webpack --config ./webpack.config.js --env.name stage",
    "build:prod": "webpack --config ./webpack.config.js --env.name prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexisK/starter-basic.git"
  },
  "keywords": [
    "javascript",
    "css",
    "sass",
    "webpack"
  ],
  "author": "AlexisK",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlexisK/starter-basic/issues"
  },
  "homepage": "https://github.com/AlexisK/starter-basic#readme",
  "devDependencies": {
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.4",
    "extract-text-webpack-plugin": "2.1.2",
    "html-webpack-plugin": "2.28.0",
    "node-sass": "4.5.3",
    "postcss-cssnext": "2.11.0",
    "postcss-import": "10.0.0",
    "postcss-loader": "2.0.6",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "sugarss": "1.0.0",
    "webpack": "2.6.1"
